     code : |1-
      // Syntax:
      //[ <Result> = ] InfoUsers ()
      //
      // This procedure is used to retrieve and lay out the information about the users
      // It returns a string formatted for the display in the example
      
      PROCEDURE InfoUsers()
      
      
      nSubscript is int
      sStringTempInfo is string
      sAnalyzeString is string
      sATempInfo is string
      
      sStringTempInfo = GetUserInfo()
      // 	string: Caption + TAB + Description + TAB + MAC Address + TAB + DNS Name
      
      // Title of the part
      sAnalyzeString += CR + TAB + <§$0019§>
      sAnalyzeString += CR + CR
      
      // Browse the results and refresh the return string
      nSubscript = 1
      FOR EACH string sATempInfo of sStringTempInfo SEPARATED BY CR
      	
      	IF NoSpace(ExtractString(sATempInfo, 1)) <> "" THEN
      		// Refresh the return string
      		sAnalyzeString += StringBuild("%1°) %2 (%3)", nSubscript, ExtractString(sATempInfo, 1), (ExtractString(sATempInfo, 2) = EOT ? "n/a" ELSE ExtractString(sATempInfo,2)))
      		sAnalyzeString += CR + CR
      		nSubscript ++
      	END
      	
      END
      
      
      RESULT sAnalyzeString
