     name : InfoOperatingSystem
     procedure_id : 1760453072552242401
     type_code : 14
     code : |1-
      // Syntax:
      //[ <Result> = ] InfoOperatingSystem ()
      //
      // This procedure is used to retrieve and lay out the information about the operating system
      // It returns a string formatted for the display in the example
      
      PROCEDURE InfoOperatingSystem()
      
      sStringTempInfo is string
      sAnalyzeString is string
      
      
      sStringTempInfo = GetInfoOS()
      // 	string: Name of Operating System +TAB + Service Pack + TAB + Official Version + TAB + Serial Number + TAB + Setup Path + TAB 
      // 			+ Setup DateTime + TAB + Reboot DateTime + TAB + Current User + TAB
      
      IF sStringTempInfo = "" THEN RESULT ""
      
      
      // Title of the part
      sAnalyzeString += CR + TAB + <§$000f§>
      sAnalyzeString += CR + CR
      
      // Update the information
      
      // Full caption of the operating system
      sAnalyzeString += StringBuild(<§$0010§>, ExtractString(sStringTempInfo, 1), ExtractString(sStringTempInfo, 2))
      sAnalyzeString += CR 
      
      // Version
      sAnalyzeString += StringBuild(<§$0011§>, ExtractString(sStringTempInfo, 3))
      sAnalyzeString += CR
      
      // Serial number
      sAnalyzeString += StringBuild(<§$0012§>, ExtractString(sStringTempInfo, 4))
      sAnalyzeString += CR 
      
      // Setup path
      sAnalyzeString += StringBuild(<§$0013§>, ExtractString(sStringTempInfo, 5))
      sAnalyzeString += CR 
      
      // Setup Date
      sAnalyzeString += StringBuild(<§$0014§>, DateToString(ExtractString(sStringTempInfo, 6)[[ TO 8]], <§$0015§>),...
      TimeToString(ExtractString(sStringTempInfo,6)[[9 TO]],<§$0016§>))
      sAnalyzeString += CR 
      
      // Reboot Date
      sAnalyzeString += StringBuild(<§$0017§>, DateToString(ExtractString(sStringTempInfo, 7)[[ TO 8]], <§$0015§>),...
      TimeToString(ExtractString(sStringTempInfo,7)[[9 TO]],<§$0016§>))
      sAnalyzeString += CR
      
      // Current user
      sAnalyzeString += StringBuild(<§$0018§>, ExtractString(sStringTempInfo, 8))
      sAnalyzeString += CR
      
      
      RESULT sAnalyzeString
