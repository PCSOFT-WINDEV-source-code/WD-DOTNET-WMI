      // Syntax:
      //[ <Result> = ] InfoNetwork ()
      //
      // This procedure is used to retrieve and lay out the information about the network
      // It returns a string formatted for the display in the example
      
      PROCEDURE InfoNetwork()
      
      sStringTempInfo is string
      sAnalyzeString is string
      sATempInfo is string
      
      sStringTempInfo = GetNetworkInfo()
      // 	string: Caption + TAB + Description + TAB + MAC Address + TAB + DNS Name
      
      // Title of the part
      sAnalyzeString += CR + TAB + <§$000b§>
      sAnalyzeString += CR + CR
      
      // Browse the results and refresh the return string
      FOR EACH string sATempInfo of sStringTempInfo SEPARATED BY CR
      	IF NoSpace(ExtractString(sATempInfo, 1)) <> "" THEN
      		
      		// Refresh the return string
      		sAnalyzeString += StringBuild(<§$000c§>, ExtractString(sATempInfo, 1), ExtractString(sATempInfo, 2))
      		sAnalyzeString += CR
      		IF NOT (NoSpace(ExtractString(sATempInfo, 3)) IN ("",EOT)) THEN
