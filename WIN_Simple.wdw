     procedure_id : 1760453072552045793
     type_code : 14
     code : |1-
      // Syntax:
      //[ <Result> = ] InfoDiskDrives ()
      //
      // This procedure is used to retrieve and lay out the information about the drives installed on the computer
      // It returns a string formatted for the display in the example
      
      PROCEDURE InfoDiskDrives()
      
      
      sStringTempInfo is string
      sAnalyzeString is string
      sATempInfo is string
      
      sStringTempInfo = GetDriveInfo()
      // Drive Letter + TAB + Description +TAB + Volume Name + TAB + File System + TAB + Total Space + TAB +
      //  Available Space + TAB + Provider Name
      
      
      // Title of the part
      sAnalyzeString += CR + TAB +<§$0002§>
      sAnalyzeString += CR + CR
      
      // Browse the return string to retrieve the information about the different disks
      FOR EACH string sATempInfo of sStringTempInfo SEPARATED BY CR
      	IF ExtractString(sATempInfo, 1) <> "" THEN 
      		
      		// Retrieve the description and the letter of the drive as well as the volume name
      		sAnalyzeString += StringBuild(<§$0003§>, ExtractString(sATempInfo, 2), ExtractString(sATempInfo, 1), "[" + (ExtractString(sATempInfo, 3) = EOT ? "n/a" ELSE ExtractString(sATempInfo, 3)) +"]")
      		
      		// Retrieve the free size and the total size
      		IF NOT (NoSpace(ExtractString(sATempInfo, 6)) IN ("", EOT)) THEN
      			sAnalyzeString += TAB
      			sAnalyzeString += StringBuild(<§$0006§>, LengthToString(ExtractString(sATempInfo, 6)), LengthToString((ExtractString(sATempInfo, 5))))
      		END
      		
      		sAnalyzeString += CR
      		// Retrieve the UNC path (network drive)
      		IF NOT (NoSpace(ExtractString(sATempInfo, 7)) IN ("", EOT)) THEN
      			
      			sAnalyzeString += StringBuild(<§$0007§>, (ExtractString(sATempInfo, 7) = 0 ? "n/a" ELSE ExtractString(sATempInfo, 7)))	
      			sAnalyzeString += CR 
      		END
      	END
      	
      	sAnalyzeString += CR 
      END
      
      
      RESULT sAnalyzeString
